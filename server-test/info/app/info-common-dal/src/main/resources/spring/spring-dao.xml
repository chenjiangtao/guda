	<bean id="imgMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">
		<property name="mapperInterface"
			value="com.foodoon.info.common.dal.ImgMapper" />
		<property name="sqlSessionFactory" ref="sqlSessionFactory" />
	</bean>
		


</beans>
